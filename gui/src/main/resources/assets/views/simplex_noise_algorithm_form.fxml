<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.GridPane?>

<TitledPane text="Algorithm parameters"
            stylesheets="@../css/main.css"
            xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="by.babanin.genesis.gui.controller.SimplexNoiseAlgorithmFormController">
    <GridPane styleClass="form-pane">
        <Label text="Offset of terrain generator:" GridPane.rowIndex="0" GridPane.columnIndex="0" />
        <TextField fx:id="offsetAlgorithmField" GridPane.rowIndex="0" GridPane.columnIndex="1" />
    </GridPane>
</TitledPane>
